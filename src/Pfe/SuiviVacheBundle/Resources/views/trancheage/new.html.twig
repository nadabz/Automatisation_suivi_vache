{% extends 'layout_admin.html.twig' %}
{% form_theme form 'bootstrap_3_layout.html.twig' %}
{% block body %}
    <h2>Ajouter une tranche d'âge</h2>

    <div class="row marg_bot_20">
    <div class="col-md-12">
        <ul class="liste_btn_action">
            <li>
                <a class="btn btn-info" href="{{ path('trancheage_index') }}"><span class="fa fa-bars"></span> Les catégories d'âge</a>
            </li>
        </ul>
    </div>
</div>

    {{ form_start(form) }}
<div class="row">
    <div class="col-md-6">
        {{ form_row(form.minAge) }}
    </div>
    <div class="col-md-6">
        {{ form_row(form.maxAge) }}
    </div>
</div>
<div class="row">
    <div class="col-md-12">
<input type="submit" value="Enregistrer"  class="btn btn-success"/>
    {{ form_end(form) }}
    </div></div>

{% endblock %}
