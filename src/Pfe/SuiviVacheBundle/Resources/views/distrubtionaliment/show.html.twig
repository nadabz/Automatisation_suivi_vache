{% extends 'base.html.twig' %}

{% block body %}
    <h1>Distrubtionaliment</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ distrubtionAliment.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{% if distrubtionAliment.date %}{{ distrubtionAliment.date|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Qtedistribue</th>
                <td>{{ distrubtionAliment.qteDistribue }}</td>
            </tr>
            <tr>
                <th>Qteadistribuer</th>
                <td>{{ distrubtionAliment.qteADistribuer }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('distrubtionaliment_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('distrubtionaliment_edit', { 'id': distrubtionAliment.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
